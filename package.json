    "@types/node": "^12.7.11",
    "@types/sinon": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "chai": "^4.2.0",
    "codecov": "^3.6.1",
    "eslint": "^6.5.1",
